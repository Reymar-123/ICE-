<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".laoac_station"
    android:background="#e5e5e5">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="#27445C"
        tools:ignore="MissingConstraints">

    </androidx.appcompat.widget.Toolbar>


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/toolbar"
        app:layout_constraintVertical_bias="0.099"
        app:srcCompat="@drawable/laoaccover"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="140dp"
        android:layout_height="140dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar"
        app:layout_constraintVertical_bias="0.188"
        app:srcCompat="@drawable/laoac1" />


    <RelativeLayout
        android:id="@+id/station_title"
        android:layout_width="254dp"
        android:layout_height="100dp"
        android:background="@drawable/shape"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.05">

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="65dp"
            android:layout_marginTop="42dp"
            android:text="Laoac Station"
            android:textColor="#27445C"
            android:textSize="20sp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/texview2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="78dp"
            android:text="Name of Station"
            android:textColor="#27445C"
            android:textSize="13sp" />

        <ImageView
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_marginStart="150dp"
            android:src="@drawable/yticon" />

        <ImageView
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_marginStart="115dp"
            android:src="@drawable/twiitericon" />

        <ImageView
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_marginStart="80dp"
            android:src="@drawable/fbicon" />

        <ImageView
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:layout_marginStart="45dp"
            android:src="@drawable/igicon" />


    </RelativeLayout>

    <RelativeLayout
        android:layout_width="290dp"
        android:layout_height="290dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/shape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/station_title"
        app:layout_constraintVertical_bias="0.045">

        <ImageView
            android:id="@+id/email_icon"
            android:layout_width="21dp"
            android:layout_height="21dp"
            android:layout_marginLeft="135dp"
            android:layout_marginTop="8dp"
            android:src="@drawable/ic_baseline_email_24" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/email_icon"
            android:layout_marginLeft="100dp"
            android:text="adsasdsdssds"
            android:textColor="#27445C"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView3"
            android:layout_marginLeft="105dp"
            android:text="Email Address"
            android:textColor="#27445C"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/phone_icon"
            android:layout_width="21dp"
            android:layout_height="21dp"
            android:layout_below="@+id/textView4"
            android:layout_marginLeft="135dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/ic_baseline_phone_android_24" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/phone_icon"
            android:layout_marginLeft="90dp"
            android:text="+6391234567890"
            android:textColor="#27445C"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView5"
            android:layout_marginLeft="105dp"
            android:text="Contact Number"
            android:textColor="#27445C"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/user_icon"
            android:layout_width="21dp"
            android:layout_height="21dp"
            android:layout_below="@+id/textView6"
            android:layout_marginLeft="135dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/ic_baseline_person_pin_24" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/user_icon"
            android:layout_marginLeft="105dp"
            android:text="Person Name"
            android:textColor="#27445C"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView7"
            android:layout_marginLeft="85dp"
            android:text="City/Municipal Marshal"
            android:textColor="#27445C"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/location_icon"
            android:layout_width="21dp"
            android:layout_height="21dp"
            android:layout_below="@+id/textView8"
            android:layout_marginLeft="135dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/ic_baseline_location_on_24" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/location_icon"
            android:layout_marginLeft="90dp"
            android:text="Laoac, Pangasinan"
            android:textColor="#27445C"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView9"
            android:layout_marginLeft="125dp"
            android:text="Location"
            android:textColor="#27445C"
            android:textSize="12sp" />





    </RelativeLayout>

    <ImageView
        android:id="@+id/visiticonlaoac"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:src="@drawable/visiticon1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.186"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.975" />

    <ImageView
        android:id="@+id/calliconlaoac"
        android:layout_width="166dp"
        android:layout_height="124dp"
        android:src="@drawable/callicon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.934"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>